{
   "version": 2,
   "name": "music-album",
   "builds": [
      { "src": "backend/app.js", "use": "@vercel/node" }
   ],
   "routes": [
        {
            "src": "/(.*)",
            "dest": "backend/index.js",
            "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
        }
    ]
}
